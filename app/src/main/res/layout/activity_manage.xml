<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MyPage.Manage.ManageActivity">

    <TextView
        android:id="@+id/textview_offfering_confirm"
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:layout_marginStart="43dp"
        android:fontFamily="@font/nbold"
        android:text="헌금 내역 확인"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/view8"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view8"
        android:layout_width="330dp"
        android:layout_height="1dp"
        android:layout_marginTop="65dp"
        android:background="#CCCBCB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.53"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_password_change" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="45dp"
        android:text=">"
        android:textColor="#CCCBCB"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/textview_withdraw"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/view9"
        android:layout_width="330dp"
        android:layout_height="1dp"
        android:layout_marginTop="65dp"
        android:background="#CCCBCB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <View
        android:id="@+id/view6"
        android:layout_width="330dp"
        android:layout_height="1dp"
        android:layout_marginTop="90dp"
        android:background="#CCCBCB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view7"
        android:layout_width="330dp"
        android:layout_height="1dp"
        android:layout_marginTop="65dp"
        android:background="#CCCBCB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_offfering_confirm" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="45dp"
        android:text=">"
        android:textColor="#CCCBCB"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/view6"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="45dp"
        android:text=">"
        android:textColor="#CCCBCB"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="45dp"
        android:text=">"
        android:textColor="#CCCBCB"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/textview_offfering_confirm"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textview_password_change"
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:layout_marginStart="43dp"
        android:fontFamily="@font/nbold"
        android:text="비밀번호 변경"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/view6"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textview_terms_confirm"
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:layout_marginStart="43dp"
        android:fontFamily="@font/nbold"
        android:text="약관 확인"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textview_withdraw"
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:layout_marginStart="43dp"
        android:fontFamily="@font/nbold"
        android:text="회원탈퇴"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/view9"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>